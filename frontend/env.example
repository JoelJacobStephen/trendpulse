# TrendPulse Frontend - Environment Configuration
# Copy this file to .env.local and update with your settings

# Backend API URL
VUE_APP_API_URL=http://localhost:8000/api/v1

# Application Title
VUE_APP_TITLE=TrendPulse

# Debug mode (optional)
VUE_APP_DEBUG=true

# Auto-refresh interval in milliseconds (optional)
VUE_APP_REFRESH_INTERVAL=300000 